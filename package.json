  "type": "module",
  "scripts": {
    "dev": "vite --port 8080 --strictPort",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ci": "vitest run --reporter=junit --outputFile=reports/junit.xml && bdy tests upload --format junit-xml reports/junit.xml",
    "cy": "xvfb-run -a cypress run --e2e --browser electron",
    "cy:ci": "start-server-and-test 'sh -c \"curl -sf http://127.0.0.1:8080/@vite/client || npm run dev\"' http://127.0.0.1:8080/@vite/client 'ELECTRON_DISABLE_GPU=1 xvfb-run -a cypress run --e2e --browser electron' && bdy tests upload --format junit-xml \"reports/cypress-*.xml\"",
    "ci": "npm run test:ci && npm run cy:ci",
    "free8080": "kill-port 8080",
    "free8080hard": "bash -lc \"fuser -k 8080/tcp || true; pkill -f 'vite|node.*vite' || true; (command -v lsof >/dev/null && lsof -tiTCP:8080 -sTCP:LISTEN | xargs -r kill -9) || true\"",
    "precy:ci": "bash -lc '[[ \"$CI\" == \"true\" ]] && npm run free8080hard || true'"
  },
